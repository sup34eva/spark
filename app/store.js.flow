// @flow
import type {
    State,
} from './reducers';

declare class Action {
    type: string,
    payload?: any,
    error?: boolean,
}

declare function Dispatch(action: Action): any;
declare function GetState(): State;

declare class Middleware {
    dispatch(action: Action): any,
    getState(): State,
}

declare class Store mixin Middleware {
    subscribe(listener: () => void): () => void,
}
