  "scripts": {
    "install": "flow-typed install",
    "start": "concurrently --kill-others -p \"[{name}]\" -n \"SERVER,CLIENT,RELAY\" -c \"bgRed.bold,bgBlue.bold,bgGreen.bold\" \"npm run server\" \"npm run client -- dev\" \"npm run relay -- --watch\"",
